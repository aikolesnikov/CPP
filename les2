#include <iostream>
#include "11.cpp"

using namespace std;

int main()
{
int a, b;
cin >> a >> b ; cout << endl;

//for (int i=0; i<a; ++i){
//    for (int j=0; j<b; ++j)
//            cout << "*";
//    cout << endl;
//}

//for (int i=0; i<a; ++i){
//    for (int j=0; j<=b; ++j)
//            cout << "*";
//    b--;
//    cout << endl;
//}

int bb=b; int k;
for (int i=0; i<a; ++i){
    for (int j=1; j<=bb; ++j){
        if (j<bb)
            cout << " ";
        else
            for (k=j-1; k<b; ++k)
               cout << "*";
    }
    bb--;
    cout << endl;
}

    return 0;

}
